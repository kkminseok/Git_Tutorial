#include<string>
#include<iostream>
#include<algorithm>
#include<stdio.h>
#include<stdlib.h>
#include<functional>
#include<cmath>
#include<ctime>
#include<vector>
using namespace std;


int main()
{
	int t;
	scanf("%d",&t);
	getchar();
	for(int i=0;i<t;++i)
	{
		int n,m;
		scanf("%d %d",&n,&m);
		int sum=m;
		int u=m;
		int t=0;
		for(int j=0;j<m;++j)
		{
			if(sum==n)
				break;
			sum++;
			++t;
			--u;
		}
		printf("%d %d\n",u,t);
	}
	
	
}
