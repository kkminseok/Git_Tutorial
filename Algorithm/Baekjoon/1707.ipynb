from collections import deque
def BFS(start,li,mapz):
    queue = deque([start])
    mapz[start] = 1
    while queue:
        curr = queue.popleft()
        for i in range (0,len(li[curr])):
            ver = li[curr][i]
            if mapz[ver] != 0:
                if mapz[curr] == mapz[ver]:
                    return False
            else : 
                queue.append(ver)
                mapz[ver] = 3-mapz[curr]
    return True

t = int(input())

